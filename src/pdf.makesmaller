#!/bin/bash

# The script shrinks the size of PDF file.
#   pdf.makesmaller <input file> <output file>

# Skriptas sumažinantis PDFo dydį.
#   pdf.makesmaller <įvesties failas> <išvesties failas>

set -x
gs -dNOPAUSE -dBATCH -sDEVICE=pdfwrite -dCompatibilityLevel=1.4 -dPDFSETTINGS=/ebook -sOutputFile="$2" "$1"
